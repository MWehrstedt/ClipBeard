{
"manifest_version": 2,
    "name": "ClipBeard",
	"author": "MWehrstedt",
    "description": "Show frequently used snippets",
    "version": "0.0.0.1",
    "icons":{
        "24":"icons/icon-24.png",
        "44":"icons/icon-44.png"
    },
    "browser_action": {
        "default_icon": {
            "20": "icons/icon-20.png",
            "40": "icons/icon-40.png"
        },

        "default_title": "ClipBeard",
        "default_popup": "canvas.html"
    }
}